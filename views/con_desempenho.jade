							option(value="1") Opci&oacute;n 1
							option(value="2") Opci&oacute;n 2
							option(value="3") Opci&oacute;n 3 
							option(value="4") Opci&oacute;n 4 
							option(value="1") Opci&oacute;n 5 
					div.text-center.col-md-2
						input.pasar.izq.form-control(type="button",value="Pasar >>")
						input.quitar.der.form-control(type="button",value="<< Quitar")
						br
						input.pasartodos.izq.form-control(type="button",value="Todos >>>")
						input.quitartodos.der.form-control(type="button",value="<<< Todos")
					div.text-center.col-md-5
						select.form-control(name="destino[]",id="destino",multiple="multiple",size="8")
